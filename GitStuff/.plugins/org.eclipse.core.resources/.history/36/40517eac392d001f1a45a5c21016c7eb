package coreJava;

public class Constructor {
	
	public Constructor()
	{
		System.out.println("I am the non parameterized one");
	}
	
	//Parameterized Constructor
	public Constructor(int a,int b)
	{
		System.out.println("I am the parameterized one" + (a+b));
	}
	public static void main (String[] args)
	{
		Constructor c=new Constructor();
		Constructor c=new Constructor(3,6);
		c.getData();
	}
	
	public void getData()
	{
		System.out.println("Always execute me");
	}

}
