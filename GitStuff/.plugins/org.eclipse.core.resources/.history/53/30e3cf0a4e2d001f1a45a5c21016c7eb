package coreJava;

public class StaticVar {
	
	//Instance Variables
	String name;
	String address;
	static String city="Hyderabad"; // static variables
	static int i=0;
	
	//static block
	static {
		city="Hyderabad"; 
		 i=0;
	}
	
	//Constructor
	StaticVar(String name,String street) // local variables
	{
		this.name=name;
		this.address=street;
		i++;
		System.out.println(i);
	}
	
	public void getData()
	{
		System.out.println(address+" "+city);
	
	}
	public static void main (String[] args)
	{
		StaticVar obj=new StaticVar("Raj","Kapra");
		obj.getData();
		StaticVar obj1=new StaticVar("Himan","Vendi");
		obj1.getData();
		
	}
	

}
