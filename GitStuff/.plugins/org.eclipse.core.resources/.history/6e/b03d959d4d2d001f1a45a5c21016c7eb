package coreJava;

public class StaticVar {
	
	//Instance Variables
	String name;
	String address;
	String city;
	
	
	//Constructor
	StaticVar(String name,String street,String city) // local variables
	{
		this.name=name;
		this.address=street;
		this.city=city;
	}
	
	public void getData()
	{
		System.out.println(address);
	}
	public static void main (String[] args)
	{
		StaticVar obj=new StaticVar("Raj","Kapra","Hyderabad");
		StaticVar obj1=new StaticVar("Himan","Vendi","Hyderabad");
		obj.getData();
		obj1.getData();
		
	}
	

}
