package coreJava;

public class Constructor {
	
	public Constructor()
	{
		System.out.println("I am the non parameterized one");
	}
	
	public static void main (String[] args)
	{
		Constructor c=new Constructor();
		c.getData();
	}
	
	public void getData()
	{
		System.out.println("Always execute me");
	}

}
