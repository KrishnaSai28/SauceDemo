package coreJava;

public class IndiaTraffic implements CentralTraffic {
	
	public static void main(String[] args)
	{
		CentralTraffic a=new IndiaTraffic();
		a.redstop();
		a.greengo();
		a.yellowwait();
		
		
	}

	public void walking()
	{
		System.out.println("Please walk");
	}
	@Override
	public void greengo() {
		// TODO Auto-generated method stub
		System.out.println("Good to go");
		
	}

	@Override
	public void redstop() {
		// TODO Auto-generated method stub
		System.out.println("need to stop");
	}

	@Override
	public void yellowwait() {
		// TODO Auto-generated method stub
		System.out.println("just wait");
	}

}
